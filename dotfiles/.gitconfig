[user]
	name = Jonathan Too
	email = jonathanjtoo@gmail.com
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[color]
	branch = auto
	diff = auto
	status = auto
	showbranch = auto
	ui = true
[color "status"]
	added = green
	changed = cyan
	untracked = magenta
[color "diff"]
	meta = cyan bold
	frag = magenta bold
[push]
#	default = simple
    default = current
[alias]
	superclean = "!f(){ git clean -xffd && git checkout .;};f"
	pwd = !pwd
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	logone = log --oneline
